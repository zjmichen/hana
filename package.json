{
  "name": "transformy",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "start": "cross-env ENV=development electron .",
    "dev": "concurrently -k \"babel-node server.js\" \"npm start\"",
    "build": "webpack --progress --colors --config webpack.config.production.js && electron-packager . Transformy --platform=linux --arch=x64 --overwrite --out dist/ && npm run package",
    "package": "electron-installer-debian --src dist/Transformy-linux-x64/ --dest dist/installers/ --arch amd64"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ]
  },
  "author": "Zack Michener",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.1",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "concurrently": "^2.1.0",
    "cross-env": "^1.0.8",
    "css-loader": "^0.23.1",
    "electron-installer-debian": "^0.3.0",
    "electron-packager": "^7.0.3",
    "electron-prebuilt": "^1.2.0",
    "express": "^4.13.4",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0",
    "webpack-target-electron-renderer": "^0.4.0"
  },
  "dependencies": {
    "csv": "^1.1.0"
  }
}
